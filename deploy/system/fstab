proc            /proc            proc    defaults 0 0
/dev/mmcblk0p1  /boot            vfat    defaults,ro 0 0
/dev/mmcblk0p2  /                ext4    defaults,noatime,ro 0 0
/dev/mmcblk0p3  /storage         ext4    defaults,noatime,sync 0 0
tmpfs           /tmp             tmpfs   defaults,noatime,nosuid,nodev,noexec,mode=1777,size=128M 0 0
tmpfs           /var/tmp         tmpfs   defaults,noatime,nosuid,noexec,nodev,mode=1777,size=128M 0 0
